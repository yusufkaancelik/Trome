{
  "expo": {
    "name": "Trome App",
    "slug": "Trome-App",
    "scheme": "tromeapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
